{
	"title": "Make a template or partial extend another partial",
	"output":[
		"",
		"",
		"",
		"<html><head><style>body {border: 3px solid;}</style><title>Products : potatoes</title></head><body><header><h1>Products : potatoes</h1></header><section class='product'><p>Hey! Please buy potatoes.</p></section></body></html>"
	],
	"input":[
		"{partial 'html'}<html><head><style>body {{border: 3px solid;}}</style>{=head/}</head><body>{=body/}</body></html>{/}",

		"{partial 'main' >> render 'html'}{set 'head'}<title>{=title /}</title>{/}{set 'body'}<header>{=header /}</header>{=content /}{/}{/}",

		"{partial 'product' >> render 'main'}{set 'title'}Products : {=title/}{/}{set 'header'}<h1>{=title/}</h1>{/}{set 'content'}<section class='product'>{=content /}</section>{/}{/}",

		"{render 'product'}{set 'title'}potatoes{/}{set 'content'}<p>Hey! Please buy potatoes.</p>{/}{/}"
	]
}